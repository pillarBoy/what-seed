{
  "name": "webpack-seed",
  "version": "1.0.0",
  "sideEffects": false,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "fuck-env NODE_ENV=dev webpack-dev-server --mode=development --inline --config webpack.config.js --progress --color",
    "start:zh": "fuck-env NODE_LANG=zh yarn start",
    "start:en": "fuck-env NODE_LANG=en yarn start",
    "build": "fuck-env NODE_ENV=prod webpack --config webpack.config.js --mode=production --progress --color",
    "eslintfix": "eslint --fix src",
    "i18nScript": "node ./i18n/script.js",
    "i18n": "fuck-env i18n=create NODE_LANG=zh yarn build",
    "build:i18n": "fuck-env i18n=build yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^8.0.1",
    "fuck-env": "^0.3.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "px2rem-loader": "^0.1.9",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "underscore-template-loader": "^1.0.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "copy-webpack-plugin": "^5.0.3"
  },
  "dependencies": {
  },
  "browserslist": [
    "> .5%",
    "last 30 versions"
  ]
}
